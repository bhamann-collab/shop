<div class="row">
    <div class="col-md-12">
        {{#each user}}
        <h1>Welcome {{this.dataValues.name}}</h1>
        {{#each this.dataValues.orders}}
        <h3>Order Id: {{this.dataValues.order_code}}</h3>
        <div>
            {{#each this.dataValues.items}}
            <ul>

                <p><button data-id="{{dataValues.id}}" type="button" class="btn btn-danger delete-itemCart">X</button>
                    {{dataValues.saleItem.dataValues.name}}: ${{dataValues.saleItem.dataValues.price}}<span
                        class="price"></span></p>
            </ul>
            {{/each}}
        </div>
        {{/each}}
        {{/each}}
    </div>
</div>


<script src="/js/script.js"></script>